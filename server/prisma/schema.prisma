generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  //url      = env("DATABASE_URL")
  url      = env("DATABASE_URL")
}

model User {
  id          String    @default(uuid()) @id
  full_name   String
  username    String    @unique
  email       String    @unique 
  password    String
  created_at  DateTime  @default(now())
}